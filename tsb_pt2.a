	cmp joywert
	beq +
	cpx #8
	bne -

	lda #0		; not in tab, so $00
	sta joywert
	beq ++

+	stx joywert	; we have a joy value

++	lda $20		; now isolate fire
	and #$10
	bne +
	lda joywert	; and indicate in value
	ora #$80
	sta joywert

+	ldy joywert	; convert to float
	jmp sxyflp3	; $8CCB

