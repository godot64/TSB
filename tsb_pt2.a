; ----------------------------------------- Part of PLAY
; 8a56
sirqplay	lda musicz4	; from sirq5
	beq sip0
	dec musicz2
	lda musicz2
	beq sip1
+	rts

sip1	dec musicz4
	lda musicz4
	beq sip0
	lda musicz3
	sta musicz2
	rts

sip0	lda musicadr	; get adr of music string
	sta $fc
	lda musicadr+1
	sta $fd
	ldy musicz1	; get pointer into string
	cpy musiclen	; at end of string?
	bcs sip2		; yes, end PLAY

	lda ($fc),y
	cmp #$8d		; beyond f8? (code for longest note)
	bcs sip9		; yes
	cmp #$85		; below f1? (code for shortest note)
	bcc sip9		; yes
	sbc #$84		; code valid, set individ. note duration
	sta musicz2
	sta musicz3
	lda musicdau
	sta musicz4
	bne sip4		; uncond. branch
; 8a9f
sip2	sty musicz5	; store pointer into string
	jsr notesuch	; search note, result in .a and .x
	ldy #1
	sta ($fe),y
	txa
	dey
	sta ($fe),y
	jsr stimmein	; play on
	ldy musicz5	; re-get pointer

sip4	cpy musiclen	; at end of music string?
	bcs playend	; yes
	iny		; no, advance
	sty musicz1
	rts
; 8abd
playend	lda #0		; clear play flag
	sta musicz1
	sta playflag
	rts

sip9	cmp #$93		; clr (code for select voice)?
	bne sip2		; no, note?
	iny		; yes, get voice number
	lda ($fc),y
	ldx #$00
	stx musicz4
	cmp #$33		; beyond voice 3?
	bcs sip5
	cmp #$31		; below voice 1?
	bcc sip5
	sbc #$30		; make value from voice #
	tax
	dex
	lda #$d4		; set $d4xx (from voice)
	sta mregadr+1	; 
	lda mregs,x	; pointer to voice 1..3
	sta mregadr
	stx stimmenr
-	jmp sip4		; advance in music string
; 8aef
sip5	cmp #$47		; code "g": voice off?
	bne sip3
	tya
	pha
	jsr svmregadr
	ldy stimmenr
	lda wavetab,y
	and #$fe
	ldy #4		; same code as below!
sip11	sta ($fe),y
	pla
	tay
	bne -		; advance
; 8b08
sip3	cmp #$54		; code "t": sync bit?
	bne sip6
	tya
	pha
	jsr svmregadr
	ldy stimmenr
	lda wavetab,y
	eor #$02
--	ldy #4
	bne sip11		; advance
; 8b1d
sip6	cmp #$43		; code "c": clear waveform
	bne sip7
	tya
	pha
	jsr svmregadr
	lda #0
	beq --

sip7	cmp #$52		; code "r": repeat music
	bne -
	ldy #0
	sty musicz1
	sty musicz4
	rts

