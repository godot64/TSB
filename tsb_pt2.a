	beq +		; .y=0

	jsr $aefa		; yes, "("?
	jsr sgetbyt1	; value to .x
	jsr schkklz	; ")"?
	txa
	and #1		; delimit to 0/1
	tay

+	lda (helpvec),y	; get joy
	sta $20
	and #$6f		; skip fire
	sta joywert

	ldx #0
-	lda tabjoy,x	; convert to joy value
